{
  "intents": [
    {
      "tag": "greeting",
      "patterns": [
        "Hi",
        "Hii",
        "hi",
        "hii",
        "Hey",
        "How are you",
        "Is anyone there?",
        "Hello",
        "Good day"
      ],
      "responses": [
        "Hey :-)",
        "Hello, thanks for visiting",
        "Hi there, what can I do for you?",
        "Hi there, how can I help?"
      ]
    },
    {
      "tag": "goodbye",
      "patterns": ["Bye", "See you later", "Goodbye","by","quit"],
      "responses": [
        "See you later, thanks for visiting",
        "Have a nice day",
        "Bye! Come back again soon."
      ]
    },
    {
      "tag": "thanks",
      "patterns": ["Thanks", "Thank you", "That's helpful", "Thank's a lot!","good","ok","okay"],
      "responses": ["Happy to help!", "Any time!", "My pleasure"]
    },
    {
      "tag": "job",
      "patterns": [
        "What can you do?",
        "What do you do?",
        "How you can help me?",
        "how can you support me?",
        "Which service you can provide"
      ],
      "responses": [
        "I can give you information like what item you can buy,how you can pay and duration of delivery",
        "I can guide you about our Tea and coffee sell services"
      ]
    },
    {
      "tag": "items",
      "patterns": [
        "Which items do you have?",
        "What kinds of items are there?",
        "What do you sell?"
      ],
      "responses": [
        "We sell coffee and tea",
        "We have coffee and tea"
      ]
    },
    {
      "tag": "payments",
      "patterns": [
        "Do you take credit cards?",
        "Do you accept Mastercard?",
        "Can I pay with Paypal?",
        "Are you cash only?"
      ],
      "responses": [
        "We accept VISA, Mastercard and Paypal",
        "We accept most major credit cards, and Paypal"
      ]
    },
    {
      "tag": "online_payments",
      "patterns": [
        "Do you take online payments?",
        "Do you accept online payments?",
        "online payment",
        "accept online payments?"
      ],
      "responses": [
        "We accept online payments",
        "We accept all payments methods"
      ]
    },
    {
      "tag": "price",
      "patterns": [
        "what is the price of tea?",
        "what is the price of coffee?",
        "tea price",
        "coffee price"
      ],
      "responses": [
        "Tea : Rs. 50 and Coffee : Rs 100",
        "Coffee : Rs 100 and Tea : Rs. 50"
      ]
    },
    {
      "tag": "delivery",
      "patterns": [
        "How long does delivery take?",
        "How long does shipping take?",
        "delivery",
        "shipping",
        "When do I get my delivery?"
      ],
      "responses": [
        "Delivery takes 2-4 days",
        "Shipping takes 2-4 days"
      ]
    },
    {
      "tag": "funny",
      "patterns": [
        "Tell me a joke!",
        "Tell me something funny!",
        "joke",
        "Do you know a joke?"
      ],
      "responses": [
        "Why did the hipster burn his mouth? He drank the coffee before it was cool.",
        "What did the buffalo say when his son left for college? Bison."
      ]
    }
  ]
}
